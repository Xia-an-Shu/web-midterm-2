npm install -g @nestjs/cli
npm install @faker-js/faker --save-dev
npm install pg --save
npm install --save @nestjs/typeorm typeorm

# To create
nest new midterm

# To run
npm run start:dev

# To test
npm test

# To upload to github remove local github created by nest
Remove-Item -Recurse -Force midterm\.git

# 1. Generate module
nest generate module module-name

nest g mo club
nest g mo partner
nest g mo partner-club

# 2. To create class
nest g cl name/name.entity --no-spec

nest g cl club/club.entity --no-spec
nest g cl partner/partner.entity --no-spec

# 3. To create services
nest g s name

nest g s club
nest g s partner
nest g s partner-club